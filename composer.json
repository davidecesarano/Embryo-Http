{
    "name": "davidecesarano/embryo-http",
    "type": "library",
    "description": "A PSR-7 and PSR-17 implementation for HTTP messages and factory.",
    "keywords": [
        "php",
        "psr-7",
        "psr-17",
        "http-message",
        "http-factory"
    ],
    "homepage": "https://github.com/davidecesarano/embryo-http",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Cesarano",
            "homepage": "https://github.com/davidecesarano"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/http-message": "1.0.1",
        "psr/http-factory": "1.0.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.47"
    },
    "autoload": {
        "psr-4": {
            "Embryo\\": "Embryo/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
